  }

method test2(x: Ref, y: Ref, a: Int){
  if (a > 42){
     inhale acc(x.f, 5/7)
  } else {
     inhale acc(y.f, 5/7)
  }
  //:: ExpectedOutput(assert.failed:assertion.false)
  assert perm(x.f) > 0/1
