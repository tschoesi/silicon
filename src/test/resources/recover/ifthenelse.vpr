 field f : Int

 method test1(x: Ref){
 	inhale acc(x.f, 1/3)
 	if (x.f < 3){
 		assert x.f < 3
 	} else {
 	    //:: ExpectedOutput(assert.failed:insufficient.permission)
 		assert acc(x.f, 2/3)
 	}
 	//:: ExpectedOutput(assert.failed:assertion.false)
